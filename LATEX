% Custom commands
\newcommand{\resumeItem}[2]{
  \item{
    \textbf{#1}{: #2 \vspace{-5pt}}
  }
}
%format of Subhead. first vspace measures width between straight line and phd in agr. second vspace measures distance between phd an texas a&m
\newcommand{\resumeSubheading}[2]{
  \vspace{-6pt}\item
    \begin{tabular*}{0.97\textwidth}{l@{\extracolsep{\fill}}r}
      \textbf{#1} & #2 \\
%      \textit{\small#3} & \textit{\small #4} \\
    \end{tabular*}\vspace{-1pt}
}

\newcommand{\resumeSubItem}[2]{\resumeItem{#1}{#2}\vspace{-2pt}}

\renewcommand{\labelitemii}{$\circ$}

\newcommand{\resumeSubHeadingListStart}{\begin{itemize}[leftmargin=*]}
\newcommand{\resumeSubHeadingListEnd}{\end{itemize}}
\newcommand{\resumeItemListStart}{\begin{itemize}}
\newcommand{\resumeItemListEnd}{\end{itemize}\vspace{-2pt}}

